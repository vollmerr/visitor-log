{
  "name": "visitor-log-client",
  "version": "0.0.1",
  "scripts": {
    "start:local": "env-cmd -f .env.local react-scripts start",
    "start:staging": "env-cmd -f .env.staging react-scripts start",
    "start:dev": "env-cmd -f .env.dev react-scripts start",
    "start:prod": "env-cmd -f .env.prod react-scripts start",
    "build:local": "env-cmd -f .env.local react-scripts build",
    "build:staging": "env-cmd -f .env.staging react-scripts build",
    "build:dev": "env-cmd -f .env.dev react-scripts build",
    "build:prod": "env-cmd -f .env.prod react-scripts build",
    "test": "react-scripts test --coverage --watchAll=false --transformIgnorePatterns \"node_modules/(?!(state-template))/\"",
    "test:watch": "react-scripts test --transformIgnorePatterns \"node_modules/(?!(state-template))/\"",
    "analyze": "source-map-explorer ./build/static/js/*.js"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "IE 11"
  ],
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-app-polyfill": "^1.0.1",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "redux-form": "^8.2.3",
    "redux-saga": "^1.0.3",
    "reselect": "^4.0.0",
    "state-template": "^0.3.4"
  },
  "devDependencies": {
    "env-cmd": "^9.0.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "eslint-config-airbnb": "^17.1.0",
    "mockdate": "^2.0.2",
    "react-scripts": "^3.0.1",
    "source-map-explorer": "^1.8.0"
  },
  "jest": {
    "resetMocks": true,
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
